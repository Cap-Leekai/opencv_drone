<launch>


<!--include file="$(find camera_calibrate_mono)/launch/mono_cam.launch">
</include-->

<!-- Run frame detector -->
<node pkg="opencv_drone" type="FrameDetector_v2.py" name="frame_detector_node" respawn="true" output="screen">
    <!--/d400/depth/image_rect_raw"-->
    <param name="depth_image_topic" value="/r200/depth/image_raw"/>
    
    <param name="view_result_flag" value="true"/>
    
    <param name="image_width_px" value="1280"/>
    <param name="image_height_px" value="720"/>
</node>


<!-- Run fly by line -->
<node pkg="opencv_drone" type="fly_by_line_old.py" name="fly_by_line" respawn="true" output="screen">
    
    <param name="cam_img_topic" value="/iris_rplidar/usb_cam/image_raw"/>
    
    <param name="view_result_flag" value="true"/>
    
    <param name="image_width_px" value="1280"/>
    <param name="image_height_px" value="720"/>
    
    <param name="h_trapeze" value="100"/>
    <param name="cut_tr" value="170"/>    
    
    <param name="x_forward" value="1.0"/>
    <param name="hight_center" value="1.56"/>

</node>



</launch>
